.record-page{
    width: 100vw;
    height: 100vh;
    display: flex;
    flex-direction: column;
}
.record-wrapper{
    flex-grow: 1;
    color: #fff;
}
.record-item{
    padding: 10rpx;
    margin: 15rpx;
    background: #F8952D;
    border-radius: 10rpx;
}
button{
    width: 100vw;
    border-radius: 0;
    background: #F8952D;
    color: #fff;
}